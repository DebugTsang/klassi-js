{
  "name": "template-test-suite",
  "version": "3.0.0",
  "description": "A debuggable JS BDD framework that uses nodeJs, webdriverio, JavaScript and cucumber-js",
  "Author": {
    "name": "Larry Goddard",
    "email": "larryg@klassitech.co.uk",
    "url": "linkedin.com/in/larryg"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "engines": {
    "node": ">= 8.11.3"
  },
  "main": "index.js",
  "scripts": {
    "all": "export PROJECT_NAME=template-test-suite; node ../../index.js -d ",
    "dev": "export PROJECT_NAME=template-test-suite; node ../../index.js -d -t @myNew",
    "test": "export PROJECT_NAME=template-test-suite; node ../../index.js -d -t @myNew -w browserstack/chrome",
    "stack": "export PROJECT_NAME=template-test-suite; node ../../runtime/remote/browserstack && node ../../index.js -d -w browserstack -x",
    "start": "set NODE_ENV=dev && node index.js"
  }

}
